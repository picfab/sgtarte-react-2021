{
  "tags": {
  "allowUnknownTags": true,
  "dictionaries": [
    "jsdoc",
    "closure"
  ]
  },
  "source": {
    "include": [
      "./src",
      "./README.md"
    ],
    "includePattern": ".+\\.js(doc|x)?$"
  },
  "plugins": [
    "plugins/markdown",
    "better-docs/component",
    "node_modules/better-docs/category"
  ],
  "opts": {
    "encoding": "utf8",
    "destination": "docs/",
    "recurse": true,
    "verbose": true,
    "template": "./node_modules/better-docs",
    "tutorials": "./doc-asset/tutorials"
  },
  "templates": {
    "better-docs": {
      "name": "Docs",
      "component": {
        "wrapper": "./src/wrapper-docs/Component.js"
      }
    }
  }
}
